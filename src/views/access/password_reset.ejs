<%- contentFor('title') %> Reset de senha <%- contentFor('body') %>
<main>
  <div class="login">
    <a href="/" id="login_logo_link">
      <object
        id="login_logo"
        data="/img/logo/logo_white_clean.svg"
        type="image/svg+xml"
      ></object>
    </a>

    <div class="switch">
      <a href="/login" id="link-usuario">Login de usuÃ¡rio</a>
      |
      <a href="/login" id="link-operador">Login de operador</a>
    </div>

    <br />

    <form action="" method="POST" class="login-form">
      <label for="login" class="login_form_label">Reset de senha</label>
      <input
        type="text"
        id="login"
        name="login"
        placeholder="Usuario ou email"
        class="form-control"
        required
      />

      <button type="submit" id="submit_login">Entrar</button>
    </form>
  </div>
</main>


<div class="col-12">
  <div class="panel">
    <div class="panel_title_bar">
      <h4>Usu√°rios</h4>

      <i class="material-icons md-24 md-light">settings</i>
    </div>

    <div class="y_overflow_list users">
      <% Array(30).fill().forEach((num) => { %>

      <div class="col-12 user">
        <div class="user_info">
          <img
            class="user_avatar"
            src="<%= faker.internet.avatar() %>"
            alt="user"
          />
          <div id="user_description">
            <span><%= faker.fake("{{name.findName}}") %></span>
            <span><%= faker.fake("{{internet.email}}") %></span>
            <span>Desenvolvedor</span>
          </div>
        </div>

        <div id="actions">
          <i class="material-icons md-36">edit</i>
          <i class="material-icons md-36">delete</i>
        </div>
      </div>

      <% }) %>
    </div>
  </div>
</div>
